{
    "manifest_version": 2,
    "name": "Twitch Ad Muter",
    "version": "1.1.1",
  
    "content_scripts": [
      {
        "matches": ["*://*.twitch.tv/*"],
        "js": ["content.js"]
      }
    ],

    "background": {
      "scripts": ["background.js"]
    },

    "permissions": [
      "tabs"
    ]
  }
  